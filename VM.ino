//Bitmap array management
const uint8_t* bitmaps[] = {arrowBitmap, moneyBitmap, cardBitmap, crossBitmap, checkBitmap};
const int numBitmaps = sizeof(bitmaps) / sizeof(bitmaps[0]);

void setup() {
  Serial.begin(9600);//Initialize Serial monitor
  SPI.begin();//Initialize SPI
  pinMode(in1, OUTPUT);
  pinMode(in2, OUTPUT);
  pinMode(in3, OUTPUT);
  pinMode(in4, OUTPUT);
  pinMode(ena, OUTPUT);
  pinMode(enb, OUTPUT);
  stopMotors();
  //OLED Screen
  display.initR(INITR_BLACKTAB);
  display.fillScreen(ST77XX_WHITE);
  display.setTextColor(ST77XX_BLACK);
  display.setTextSize(1);
  display.setCursor(10, 10);
  display.print("Please Select Your");
  display.print("         Order!      ");
  display.drawBitmap(20, 70, bitmaps[0], 90, 90, ST7735_BLUE); 
  delay(2000);  // Play for 2 seconds 

  pinMode(button1Pin, INPUT_PULLUP);
  pinMode(button2Pin, INPUT_PULLUP);
  // Initialized RFID 
  rfid.PCD_Init();

}

void loop() {
  // Handle Button 1
  if (digitalRead(button1Pin) == LOW) {
    display.setCursor(10, 30);
    display.fillScreen(ST77XX_WHITE);
    display.setTextSize(3);
    display.setTextColor(ST77XX_BLACK);
    display.print("         1$      "); 
    display.drawBitmap(1, 60, bitmaps[1], 128, 128, ST7735_BLACK);
    delay(3000);  // Play for 3 seconds 

    display.fillScreen(ST77XX_WHITE);
    display.setCursor(10, 30);
    display.setTextSize(1);
    display.setTextColor(ST77XX_BLACK);
    display.print(" Please Scan Card");
    display.drawBitmap(1, 60, bitmaps[2], 128, 128, ST7735_BLACK);
    delay(3000);  // Play for 3 seconds 
    if (!rfid.PICC_IsNewCardPresent() || !rfid.PICC_ReadCardSerial()) {
      return;
    }
    Serial.print("UID tag: ");
    String content = " ";
    for (byte i = 0; i < rfid.uid.size; i++) {
      content += String(rfid.uid.uidByte[i], HEX);
      if (i != rfid.uid.size - 1) content += ": ";
    }
    content.toUpperCase();
    Serial.println(content);
    bool authorized = true;
    for (byte i = 0; i < rfid.uid.size; i++) {
      if (rfid.uid.uidByte[i] != authorizedUID[i]) {
        authorized = false;
        break;
      }
    }
    if (authorized) {
      Serial.println("Authorized User:");
      display.fillScreen(ST77XX_WHITE);
      // Activate Motor 1
      digitalWrite(in1, HIGH);
      digitalWrite(in2, LOW);
      analogWrite(ena, 255);
      delay(1000);
      stopMotor1();
      // Stop Motor 2
      stopMotor2();
      display.setCursor(10, 30);
      display.setTextSize(1);
      display.setTextColor(ST77XX_BLACK);
      display.print("  Thank You for ");
      display.print("         Ordering!");
      display.drawBitmap(1, 50, bitmaps[4], 128, 128, ST7735_GREEN);
      delay(5000);
    } else {
      Serial.println("Unauthorized User: ");
      display.setCursor(10, 30);
      display.fillScreen(ST77XX_WHITE);
      display.setCursor(10, 30);
      display.setTextSize(1);
      display.setTextColor(ST77XX_BLACK);
      display.print("   Your Card is ");
      display.print("       Insufficient!");
      display.drawBitmap(1, 60, bitmaps[3], 128, 128, ST7735_RED); 
    }
    rfid.PICC_HaltA();
  }

  // Handle Button 2
  if (digitalRead(button2Pin) == LOW) {
    display.setCursor(10, 30);
    display.fillScreen(ST77XX_WHITE);
    display.setTextSize(3);
    display.setTextColor(ST77XX_BLACK);
    display.print("         2$      ");
    display.drawBitmap(1, 60, bitmaps[1], 128, 128, ST7735_BLACK);
    delay(3000);

    display.fillScreen(ST77XX_WHITE);
    display.setCursor(10, 30);
    display.setTextSize(1);
    display.setTextColor(ST77XX_BLACK);
    display.print(" Please Scan Card");
    display.drawBitmap(1, 60, bitmaps[2], 128, 128, ST7735_BLACK);
    delay(2000);
    if (!rfid.PICC_IsNewCardPresent() || !rfid.PICC_ReadCardSerial()) {
      return;
    }
    Serial.print("UID tag: ");
    String content = " ";
    for (byte i = 0; i < rfid.uid.size; i++) {
      content += String(rfid.uid.uidByte[i], HEX);
      if (i != rfid.uid.size - 1) content += ": ";
    }
    content.toUpperCase();
    Serial.println(content);
    bool authorized = true;
    for (byte i = 0; i < rfid.uid.size; i++) {
      if (rfid.uid.uidByte[i] != authorizedUID[i]) {
        authorized = false;
        break;
      }
    }
    if (authorized) {
      Serial.println("Authorized User:");
      display.setCursor(10, 30);
      display.fillScreen(ST77XX_WHITE);
      // Activate Motor 2
      digitalWrite(in3, HIGH);
      digitalWrite(in4, LOW);
      analogWrite(enb, 255);
      delay(2000);
      stopMotor2();
      // Stop Motor 1
      stopMotor1();

      display.fillScreen(ST77XX_WHITE);
      display.setCursor(10, 30);
      display.setTextSize(1);
      display.setTextColor(ST77XX_BLACK);
      display.print("  Thank You for ");
      display.print("         Ordering!");
      display.drawBitmap(1, 50, bitmaps[4], 128, 128, ST7735_GREEN);
      delay(5000);
    } else {
      Serial.println("Unauthorized User: ");
      display.fillScreen(ST77XX_WHITE);
      display.setCursor(10, 30);
      display.setTextSize(1);
      display.setTextColor(ST77XX_BLACK);
      display.print("   Your Card is ");
      display.print("       Insufficient!");
      display.drawBitmap(1, 60, bitmaps[3], 128, 128, ST7735_RED);
    }
    rfid.PICC_HaltA();
  }
}

void stopMotor1() {
  digitalWrite(in1, LOW);
  digitalWrite(in2, LOW);
  analogWrite(ena, 0);
}

void stopMotor2() {
  digitalWrite(in3, LOW);
  digitalWrite(in4, LOW);
  analogWrite(enb, 0);
}

void stopMotors() {
  stopMotor1();
  stopMotor2();
}
